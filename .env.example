# Claude Enhancer 5.1 环境配置示例
# 复制此文件为 .env 并填入实际值

# 构建信息
BUILD_DATE=2025-09-26T00:00:00Z
VCS_REF=main
VERSION=5.1.0

# 数据库配置
DB_NAME=claude_enhancer
DB_USER=claude_user
DB_PASSWORD=your-super-secure-database-password-here
DB_HOST=postgres
DB_PORT=5432

# Redis配置
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-secure-password-here

# JWT密钥 (生产环境请使用强随机密钥)
JWT_ACCESS_SECRET=your-super-secret-jwt-access-key-minimum-256-bits-long-random-string
JWT_REFRESH_SECRET=your-super-secret-jwt-refresh-key-minimum-256-bits-long-different-string

# 应用密钥
SECRET_KEY=your-super-secret-app-key-for-encryption

# 外部服务
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
DOMAIN=claude-enhancer.example.com

# 生产环境配置
CLAUDE_ENV=production
WORKERS=4
LOG_LEVEL=INFO

# Grafana配置
GRAFANA_ADMIN_PASSWORD=your-grafana-admin-password