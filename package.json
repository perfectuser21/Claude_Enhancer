{
  "name": "claude-enhancer",
  "version": "5.3.1",
  "description": "Claude Enhancer 5.3 - AI-Powered Programming Assistant",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "bdd": "cucumber-js --publish-quiet",
    "bdd:ci": "cucumber-js --format progress --publish-quiet",
    "bdd:report": "cucumber-js --format json:test/reports/cucumber_report.json",
    "lint": "eslint . --ext .js,.ts",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "validate": "bash test/validate_enhancement.sh",
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "tsc",
    "prepare": "husky install"
  },
  "keywords": [
    "claude",
    "enhancer",
    "ai",
    "programming",
    "assistant",
    "workflow",
    "bdd",
    "slo"
  ],
  "author": "Claude Enhancer Team",
  "license": "MIT",
  "devDependencies": {
    "@cucumber/cucumber": "^9.6.0",
    "@types/chai": "^4.3.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "axios": "^1.12.2",
    "chai": "^4.3.10",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "winston": "^3.11.0",
    "yaml": "^2.3.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/claude-enhancer/claude-enhancer.git"
  },
  "bugs": {
    "url": "https://github.com/claude-enhancer/claude-enhancer/issues"
  },
  "homepage": "https://github.com/claude-enhancer/claude-enhancer#readme",
  "cucumber": {
    "default": {
      "paths": [
        "acceptance/features"
      ],
      "require": [
        "acceptance/steps/**/*.js"
      ],
      "format": [
        "progress-bar",
        "html:test/reports/cucumber.html"
      ],
      "publishQuiet": true
    }
  }
}
